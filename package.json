{
  "name": "http-public",
  "version": "1.0.0",
  "description": "Forward HTTP(S) traffic from the public internet to localhost.",
  "main": "index.js",
  "scripts": {
    "lint:staged": "lint-staged",
    "test": "mocha",
    "tdd": "npm -s t -- -R min -w",
    "cover": "c8 -r text -r html npm -s t",
    "postinstall": "is-ci || husky install"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": [
      "client/**/*.ts",
      "server/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": 500
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.3",
    "@types/promise-retry": "^1.1.3",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "c8": "^7.7.0",
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "ws": "^7.4.5"
  }
}
